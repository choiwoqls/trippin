<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="nugu">
	
	<select id="selectWho" parameterType="int" resultType="String">
		select nugu_who from NUGU where nugu_seq = #{nugu_seq}
	</select>

	
</mapper>








































